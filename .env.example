# Railway SMS AT Command System Configuration

# Environment
NODE_ENV=production

# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://username:password@host:port/database

# Server Configuration
PORT=3000
WS_PORT=3001

# Development Virtual Phone
VIRTUAL_PHONE=+1234567890
ENABLE_VIRTUAL_PHONE=true

# Rate Limiting
RATE_LIMIT_PER_PHONE_PER_MINUTE=10
RATE_LIMIT_PER_PHONE_PER_HOUR=100
RATE_LIMIT_PER_PHONE_PER_DAY=500

# Security
JWT_SECRET=your_jwt_secret_here
ADMIN_USERNAME=admin
ADMIN_PASSWORD=secure_admin_password

# Bridge Connection (for local bridge)
BRIDGE_WS_URL=ws://your-bridge-server:3001
